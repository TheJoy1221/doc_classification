# .env.example

# OpenAI API Key (for GPT fallback classification)
OPENAI_API_KEY=your_openai_api_key_here

# Path to trained BERT model
MODEL_PATH=/path/to/your/trained/model

# Tesseract executable path
TESSERACT_PATH=/usr/bin/tesseract

# Input/output folders
INPUT_FOLDER=/path/to/input/pdf/folder
OUTPUT_INVOICES=/path/to/output/invoices
OUTPUT_OTHER=/path/to/output/other
ARCHIVE_FOLDER=/path/to/archive/folder
